{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8888",
      "webRoot": "${workspaceRoot}/src"
    },
    {
      "name": "Debug Jest",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "./node_modules/.bin/jest",
        "-i"
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "console": "internalConsole",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceRoot}/distribution"
      ]
    }
  ]
}
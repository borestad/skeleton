{
    "extends": "standard",
    "globals": {
        "Sys": true
    },
    "rules": {
        "eqeqeq": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "standard": {
            "computed-property-even-spacing": [
                "error",
                "never"
            ]
        }
    }
}
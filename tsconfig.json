{
  "awesomeTypescriptLoaderOptions": {
    "cacheDirectory": ".tmp/tscache",
    "silent": false,
    "transpileOnly": true,
    "useBabel": true,
    "useCache": true,
    "useTranspileModule": false,
    "forceIsolatedModules": false
  },
  "compileOnSave": true,
  "compilerOptions": {
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "inlineSourceMap": false,
    "inlineSources": true,
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "listEmittedFiles": false,
    "listFiles": false,
    "lib": ["es2017", "dom"],
    "allowJs": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": ".tmp/code",
    "pretty": true,
    "removeComments": false,
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5",
    "types": ["node", "jest"],
    "watch": false
  },
  "exclude": ["**/.tmp/**", "node_modules", "dist", ".vscode"],
  "include": ["**/*.d.ts", "**/*.ts", "**/*.tsx"]
}
